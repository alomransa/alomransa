<odoo>
    <template id="report_eos">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h2>End of Service Report</h2>
                    <p>Employee: <t t-esc="doc.employee_id.name"/></p>
                    <p>Service Years: <t t-esc="doc.service_years"/></p>
                    <p>Basic Salary: <t t-esc="doc.basic_salary"/></p>
                    <p>End of Service Amount: <t t-esc="doc.eos_amount"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>
