#. module: saudi_hr
#: model:mail.template,body_html:saudi_hr.email_template_birthday
msgid ""
"<div>\n"
"                Hi <strong><t t-out=\"object.name or ''\"/></strong>,<br/><br/>\n"
"                Wish you very Happy Birthday from <t t-out=\"object.company_id.name or ''\"/>.\n"
"                You are one of the most valuable employee of <t t-out=\"object.company_id.name or ''\"/>. \n"
"                <br/>On your precious day we wish good luck and success full year ahead.<br/>\n"
"                Party Hard Stay Safe.<br/><br/>\n"
"                Best Wishes<br/>\n"
"                <t t-out=\"object.company_id.name or ''\"/>\n"
"            </div>\n"
"        "
msgstr ""
"<div>\n"
"                Hola <strong><t t-out=\"object.name or ''\"/></strong>,<br/><br/>\n"
"                Te deseamos un feliz cumpleaños de parte de  <t t-out=\"object.company_id.name or ''\"/>.\n"
"                Eres uno de los empleados más valiosos de <t t-out=\"object.company_id.name or ''\"/>. \n"
"                <br/>En su precioso día, le deseamos buena suerte y éxito durante todo el año.<br/>\n"
"                Festeja duro y mantente a salvo.<br/><br/>\n"
"                Los mejores deseos<br/>\n"
"                <t t-out=\"object.company_id.name or ''\"/>\n"
"            </div>\n"
"        "

#. module: saudi_hr
#: model:mail.template,body_html:saudi_hr.mail_template_employee_manager_hr_evaluation
msgid ""
"<p>Hello,<br/><br/>\n"
"\n"
"                Defaulted User Information has been used, requiring your verification.<br/>\n"
"                Please make the appropriate personal changes to your account.\n"
"                <br/>\n"
"                <!-- <t out=\"\" -->\n"
"                <a t-att-href=\"ctx['access_token_url']\"><t t-out=\"ctx['access_token_url']\"/></a>\n"
"                <br/>\n"
"                <br/>\n"
"                Regards</p>\n"
"            "
msgstr ""
"<p>Hello,<br/><br/>\n"
"\n"
"                Se utilizó información por defecto, por lo que se requiere confirmación.<br/>\n"
"                Por favor realice los cambios que considere necesarios a su cuenta.\n"
"                <br/>\n"
"                <!-- <t out=\"\" -->\n"
"                <a t-att-href=\"ctx['access_token_url']\"><t t-out=\"ctx['access_token_url']\"/></a>\n"
"                <br/>\n"
