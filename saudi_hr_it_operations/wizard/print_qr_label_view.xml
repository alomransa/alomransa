<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="print_qr_label_view_form" model="ir.ui.view">
		<field name="name">print.qr.label.view.form</field>
		<field name="model">print.qr.label</field>
		<field name="arch" type="xml">
			<form>
				<group>
					<field name="label_size" widget="radio"/>
					<field name="product_ids" invisible="1"/>
				</group>
				<footer>
					<button name="confirm" type="object" string="Confirm" class="btn-primary" />
					<button string="Cancel" special="cancel" />
				</footer>
			</form>
		</field>
	</record>
</odoo>
