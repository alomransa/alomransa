<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_form_view_inherited" model="ir.ui.view">
        <field name="name">hr.employee.from.saudi.inherited</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="saudi_hr.hr_form_view" />
        <field name="arch" type="xml">
        	<xpath expr="//notebook" position="inside">
        		<page name="contract" string="Contract">
        			<field name="contract_ids" readonly="1" force_save="1" />
        		</page>
        	</xpath>
        </field>
    </record>

</odoo>
