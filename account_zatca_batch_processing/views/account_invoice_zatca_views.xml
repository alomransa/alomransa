<odoo>
    <record id="view_invoice_tree_zatca" model="ir.ui.view">
        <field name="name">account.move.zatca.tree</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="buttons">
                <button name="action_process_zatca_batch" type="object" string="Process ZATCA Invoices" class="btn-primary" />
            </xpath>
        </field>
    </record>

</odoo>
