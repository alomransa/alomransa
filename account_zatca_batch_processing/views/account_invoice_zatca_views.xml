<odoo>
<record id="view_invoice_tree_zatca" model="ir.ui.view">
    <field name="name">account.move.zatca.tree</field>
    <field name="model">account.move</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="date"/>
            <field name="amount_total"/>
