#. module: sales_warranty
#: model:mail.template,body_html:sales_warranty.warranty_running_email
msgid ""
"<p>Dear <t t-out=\"object.partner_id.name or 'Madam, Sir'\"/>,</p>\n"
"            <p>We are informing that your warranty <t t-out=\"object.warranty_no\"/> is running.</p>\n"
"            <p><t t-out=\"object.product_id.name\"/> product wararnty duration is <t t-out=\"object.start_date\"/> to <t t-out=\"object.end_date\"/>.</p>\n"
"            <p>Thank you,</p>\n"
"            <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"                <t t-out=\"object.company_id.name\"/>\n"
"            </t>\n"
"        "
msgstr ""
"<p>Estimado <t t-out=\"object.partner_id.name or 'Señora, Señor''\"/>,</p>\n"
"            <p>Le informamos que su garantía <t t-out=\"object.warranty_no\"/> está corriendo.</p>\n"
"            <p><t t-out=\"object.product_id.name\"/> la duración de la garantía del producto del <t t-out=\"object.start_date\"/> a <t t-out=\"object.end_date\"/>.</p>\n"
"            <p>Gracias,</p>\n"
"            <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"                <t t-out=\"object.company_id.name\"/>\n"
"            </t>\n"
"        "

#. module: sales_warranty
#: model:mail.template,body_html:sales_warranty.warranty_expire_email
msgid ""
"<p>Dear <t t-out=\"object.partner_id.name or 'Madam, Sir'\"/>,</p>\n"
"        <p>We are informing that your warranty <t t-out=\"object.warranty_no\"/> is expired.</p>\n"
"        <p>Thank you,</p>\n"
"        <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"            <t t-out=\"object.company_id.name\"/>\n"
"        </t>\n"
"        "
msgstr ""
"<p>Estimado  <t t-out=\"object.partner_id.name or 'Señora, Señor'\"/>,</p>\n"
"        <p>Le informamos que su garantía <t t-out=\"object.warranty_no\"/> está vencida.</p>\n"
"        <p>Gracias,</p>\n"
"        <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"            <t t-out=\"object.company_id.name\"/>\n"
"        </t>\n"
"        "

#. module: sales_warranty
#: model:mail.template,body_html:sales_warranty.warranty_expiration_email
msgid ""
"<p>Dear <t t-out=\"object.partner_id.name or 'Madam, Sir'\"/>,</p>\n"
"        <p>We are informing that your warranty <t t-out=\"object.warranty_no\"/> is near to expired please renew as soon as possible.</p>\n"
"        <p><t t-out=\"object.product_id.name\"/> product warranty end date is <t t-out=\"object.end_date\"/>.</p>\n"
"        <p>Thank you,</p>\n"
"        <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"            <t t-out=\"object.company_id.name\"/>\n"
"        </t>\n"
"        "
msgstr ""
"<p>Estimado <t t-out=\"object.partner_id.name or 'Señora, Señor'\"/>,</p>\n"
"        <p>Le informamos que su garantía <t t-out=\"object.warranty_no\"/> está a punto de caducar, renuévela lo antes posible.</p>\n"
"        <p><t t-out=\"object.product_id.name\"/> la fecha de finalización de la garantía del producto es  <t t-out=\"object.end_date\"/>.</p>\n"
"        <p>Gracias,</p>\n"
"        <t t-if=\"object.company_id.name and object.company_id.email\">\n"
"            <t t-out=\"object.company_id.name\"/>\n"
"        </t>\n"
"        "

